CombAlign.AddItem "1 - Top-Left"
CombAlign.AddItem "2 - Top-Right"
CombAlign.AddItem "3 - Middle-Center"
CombAlign.AddItem "4 - Middle-Left"
CombAlign.AddItem "5 - Middle-Right"
CombAlign.AddItem "6 - Bottom-Center"
CombAlign.AddItem "7 - Bottom-Left"
