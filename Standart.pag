Private Sub PropertyPage_ApplyChanges()
    SelectedControls(0).FontUnderline = (chkFontUnderline.Value = vbChecked)
    SelectedControls(0).FontStrikethru = (chkFontStrikethru.Value = vbChecked)
    SelectedControls(0).FontSize = txtFontSize.Text
    SelectedControls(0).FontName = txtFontName.Text
    SelectedControls(0).FontItalic = (chkFontItalic.Value = vbChecked)
    SelectedControls(0).FontBold = (chkFontBold.Value = vbChecked)
    SelectedControls(0).Caption = txtCaption.Text
    SelectedControls(0).Hit = (chkHit.Value = vbChecked)
    SelectedControls(0).Sound = (chkSound.Value = vbChecked)
    SelectedControls(0).Cursor = Trim(Mid(CombCursor.Text, 1, 2))
    SelectedControls(0).PicAlign = Trim(Mid(CombAlign.Text, 1, 2))
    SelectedControls(0).PicAlign = Trim(Mid(CombAlign.Text, 1, 2))
    SelectedControls(0).ButtonType = Trim(Mid(CombButtonType.Text, 1, 2))
    SelectedControls(0).GreyIcon = (chkGreyIcon.Value = vbChecked)

Call SelectedControls(0).ButtonRePaint
    
End Sub


Private Sub PropertyPage_SelectionChanged()
On Error Resume Next
    chkFontUnderline.Value = (SelectedControls(0).FontUnderline And vbChecked)
    chkFontStrikethru.Value = (SelectedControls(0).FontStrikethru And vbChecked)
    txtFontSize.Text = SelectedControls(0).FontSize
    txtFontName.Text = SelectedControls(0).FontName
    chkFontItalic.Value = (SelectedControls(0).FontItalic And vbChecked)
    chkFontBold.Value = (SelectedControls(0).FontBold And vbChecked)
    txtCaption.Text = SelectedControls(0).Caption
    chkHit.Value = (SelectedControls(0).Hit And vbChecked)
    chkSound.Value = (SelectedControls(0).Sound And vbChecked)
    Style(SelectedControls(0).Style).Value = True
    chkGreyIcon.Value = (SelectedControls(0).GreyIcon And vbChecked)

Select Case Trim(Mid(SelectedControls(0).Cursor, 1, 2))
Case Is = 0
CombCursor.Text = "0 - Voreinstellung"
Case Is = 1
CombCursor.Text = "1 - Pfeil"
Case Is = 2
CombCursor.Text = "2 - Kreuz"
Case Is = 3
CombCursor.Text = "3 - I-Cursor"
Case Is = 4
CombCursor.Text = "4 - Symbol"
Case Is = 5
CombCursor.Text = "5 - Größeändern"
Case Is = 6
CombCursor.Text = "6 - Größeändern NO SW"
Case Is = 7
CombCursor.Text = "7 - Größeändern NS"
Case Is = 8
CombCursor.Text = "8 - Größeändern NW SO"
Case Is = 9
CombCursor.Text = "9 - Größeändern WO"
Case Is = 10
CombCursor.Text = "10- Aufwärtpfeil"
Case Is = 11
CombCursor.Text = "11- Sanduhr"
Case Is = 12
CombCursor.Text = "12- Nicht ablegen"
Case Is = 13
CombCursor.Text = "13- Pfeil Sanduhr"
Case Is = 14
CombCursor.Text = "14- Pfeil Fragezeichen"
Case Is = 15
CombCursor.Text = "15- Größeändern alles"
Case Is = 99
CombCursor.Text = "99- Hand"
End Select

Select Case Trim(Mid(SelectedControls(0).PicAlign, 1, 2))
Case Is = 0
CombAlign.Text = "0 - Top-Center"
Case Is = 1
CombAlign.Text = "1 - Top-Left"
Case Is = 2
CombAlign.Text = "2 - Top-Right"
Case Is = 3
CombAlign.Text = "3 - Middle-Center"
Case Is = 4
CombAlign.Text = "4 - Middle-Left"
Case Is = 5
CombAlign.Text = "5 - Middle-Right"
Case Is = 6
CombAlign.Text = "6 - Bottom-Center"
Case Is = 7
CombAlign.Text = "7 - Bottom-Left"
Case Is = 8
CombAlign.Text = "8 - Bottom-Right"
End Select

Select Case Trim(Mid(SelectedControls(0).ButtonType, 1, 2))
Case Is = 0
CombButtonType.Text = "0 - normal Button"
Case Is = 9
CombButtonType.Text = "9 - Sepperator"
End Select

On Error GoTo 0
End Sub


